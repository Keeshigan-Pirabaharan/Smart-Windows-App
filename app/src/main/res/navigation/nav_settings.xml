<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_settings">
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.smartwindowsapp.SettingsActivity.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_settingsFragment_to_manualFragment"
            app:destination="@id/manualFragment" />
    </fragment>
    <fragment
        android:id="@+id/manualFragment"
        android:name="com.example.smartwindowsapp.fragments.ManualFragment"
        android:label="ManualFragment" >
        <action
            android:id="@+id/action_manualFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
</navigation>